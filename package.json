{
  "name": "@ndlrf/pdfu",
  "version": "0.6.6",
  "description": "NDLRF - PDF resources extraction and analysis toolset",
  "tags": [
    "pdf",
    "pdf/a",
    "ISO 32000",
    "converter",
    "document",
    "extractor",
    "resource carver",
    "jpeg2000",
    "orc",
    "png",
    "svg",
    "veraPDF",
    "The Russian National Electronic library",
    "The Russian National Digital library",
    "The Russian National Library",
    "data integration"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "National electronic library of Russian Federation",
      "url": "https://rusneb.ru"
    },
    {
      "name": "Russian state library",
      "url": "https://rsl.ru"
    }
  ],
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndlrf-rnd/pdfu.git"
  },
  "bin": {
    "pdfu": "./bin/pdfu"
  },
  "directories": {
    "bin": "bin",
    "lib": "lib",
    "src": "src",
    "test": "src/__tests__"
  },
  "dependencies": {
    "argparse": "1.0.10",
    "canvas": "2.8.0",
    "cwebp-bin": "6.1.1",
    "ejs": "3.1.6",
    "highwayhash": "3.1.1",
    "hummus": "1.0.110",
    "imagemin": "8.0.0",
    "imagemin-jpeg-recompress": "7.0.0",
    "imagemin-jpegtran": "7.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-pngcrush": "7.0.0",
    "imagemin-pngquant": "9.0.2",
    "imagemin-webp": "6.0.0",
    "js-yaml": "4.1.0",
    "jsdom": "16.6.0",
    "sharp": "0.28.3",
    "lodash.defaults": "4.2.0",
    "lodash.flatten": "4.4.0",
    "lodash.isnil": "4.0.0",
    "lodash.omit": "4.5.0",
    "lodash.pick": "4.4.0",
    "lodash.uniq": "4.5.0",
    "mime-types": "2.1.31",
    "mkdirp": "1.0.4",
    "parse-numeric-range": "0.0.2",
    "pdfjs": "2.4.4",
    "pdfjs-dist": "2.9.359",
    "png-to-jpeg": "1.0.1",
    "superagent": "6.1.0",
    "xml-js": "1.6.11"
  },
  "files": [
    "dist/",
    "lib/",
    "src/",
    "bin/",
    "__tests__/"
  ],
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.6",
    "@babel/eslint-parser": "7.14.7",
    "@mapbox/node-pre-gyp": "1.0.5",
    "babel-jest": "27.0.6",
    "eslint": "7.30.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.3.6",
    "jest": "27.0.6"
  },
  "scripts": {
    "lint": "eslint ./src",
    "test": "jest ./src --coverage --passWithNoTests",
    "ci": "npm run test && npm run lint && npm run build",
    "himem": "node --max-old-space-size=8192 .",
    "build": "npm run build:clean && npm run build:babel",
    "build:contrib": "./scripts/install_contrib.sh",
    "build:babel": "babel ./src -d ./lib",
    "build:clean": "rm -rf ./lib",
    "build:watch": "webpack --config ./webpack.config.js --watch",
    "build:docker": "docker build --force-rm --no-cache -f ./Dockerfile -t pdfu:latest ."
  },
  "jest": {
    "transform": {
      ".+\\.jsx?$": "babel-jest.transformer.js"
    }
  }
}
